        <!-- VUE Fournisseurs  PERSONALISÉE-->
        <record id="vendor_view_tree" model="ir.ui.view">
            <field name="name">vendor_view_tree</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <tree string="Contacts" sample="1" multi_edit="1">
                    <field name="display_name" string="Fournisseurs"/>
                    <field name="function" column_invisible="True"/>
                    <field name="phone" class="o_force_ltr" optional="show"/>
                    <field name="mobile" optional="hide"/>
                    <field name="email" optional="show"/>
                    <field name="user_id" optional="show" widget="many2one_avatar_user" domain="[('share', '=', False)]"/>
                    <field name="city" optional="show"/>
                    <field name="country_id" optional="show" readonly="1"/>
                    <field name="vat" optional="hide" readonly="1"/>
                    <field name="supplier_type" optional="show" />
                    <field name="category_id" optional="hide" widget="many2many_tags" options="{'color_field': 'color'}"/>
                    <field name="company_id" groups="base.group_multi_company" readonly="1"/>
                    <field name="is_company" column_invisible="True"/>
                    <field name="parent_id" readonly="1" column_invisible="True"/>
                    <field name="active" column_invisible="True"/>
                </tree>
            </field>
        </record>

        <record id="vendor_view_search" model="ir.ui.view">
            <field name="name">vendor_view_search</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <search string="Search Partner">
                    <field name="name" filter_domain="['|', '|', '|', '|', ('complete_name', 'ilike', self), ('ref', '=', self), ('email', 'ilike', self), ('vat', 'ilike', self), ('company_registry', 'ilike', self)]"/>
                    <field name="parent_id" domain="[('is_company', '=', True)]" operator="child_of"/>
                    <field name="email" filter_domain="[('email', 'ilike', self)]"/>
                    <field name="phone" filter_domain="['|', ('phone', 'ilike', self), ('mobile', 'ilike', self)]"/>
                    <field name="category_id" string="Tag" operator="child_of"/>
                    <field name="user_id"/>
                    <separator/>
                    <filter string="Particuliers" name="type_person" domain="[('is_company', '=', False)]"/>
                    <filter string="Sociétés" name="type_company" domain="[('is_company', '=', True)]"/>
                    <separator/>
                    <filter string="Archivé" name="inactive" domain="[('active', '=', False)]"/>
                    <separator/>
                    <group expand="0" name="group_by" string="Group By">
                        <filter name="supplier_type" string="Catégorie Fournisseur" context="{'group_by': 'supplier_type'}"/>
                        <filter name="salesperson" string="Vendeur" domain="[]" context="{'group_by' : 'user_id'}"/>
                        <filter name="group_company" string="Société" context="{'group_by': 'parent_id'}"/>
                        <filter name="group_country" string="Pays" context="{'group_by': 'country_id'}"/>
                    </group>
                </search>
                
            </field>
        </record>


        <!-- ACTION Fournisseurs QUI A LA LISTE PERSONALISÉ-->
        <record id="action_vendor" model="ir.actions.act_window">
            <field name="name">Fournisseurs</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.partner</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="search_view_id" ref="vendor_view_search"/>
