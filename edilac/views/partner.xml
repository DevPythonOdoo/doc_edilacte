<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="product_list_state" model="ir.ui.view">
            <field name="name">product.list.state</field>
            <field name="model">product.pricelist</field>
            <field name="priority">8</field>
            <field name="inherit_id" ref="product.product_pricelist_view"/>
            <field name="arch" type="xml">
               <xpath expr="//sheet" position="before">
                   <header>
                       <button name="action_submit" class="oe_highlight" invisible="state not in ('draft')" string="Soumettre" type="object"/>
                       <button name="action_validate" groups="edilac.group_edilac_respo" class="oe_highlight" invisible="state not in ('send')" string="Valider" type="object"/>
                       <button name="action_cancel" invisible="state not in ('done','send')" string="Annuler" type="object"/>
                       <field name="state" widget="statusbar" statusbar_visible="draft,send,done"/>
                   </header>
               </xpath>
            </field>
        </record>
         <record id="customer_view_form_inherit" model="ir.ui.view">
            <field name="name">client.res.partner</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">
                <xpath expr="//span[@name='address_name']" position="replace">
                    <span class="o_form_label o_td_label" name="address_name" invisible="customer_rank > 0">
                        <field name="type" invisible="is_company" readonly="user_ids" required="not is_company"
                               class="fw-bold"/>
                        <b invisible="not is_company">Address</b>
                        <field name="customer_rank" invisible="1"/>
                        <!--field name="customer" invisible="1"/-->
                    </span>
                </xpath>

                <xpath expr="//div[@class='o_address_format']" position="replace">
                         <field name="customer_rank" invisible="1"/>
                        <div class="o_address_format" invisible="customer_rank > 0">
                            <field name="street" placeholder="Street..." class="o_address_street"
                                   readonly="type == 'contact' and parent_id"/>
                            <field name="street2" placeholder="Street 2..." class="o_address_street"
                                   readonly="type == 'contact' and parent_id"/>
                            <field name="city" placeholder="City" class="o_address_city"
                                   readonly="type == 'contact' and parent_id"/>
                            <field name="state_id" class="o_address_state" placeholder="State"
                                   options="{'no_open': True, 'no_quick_create': True}"
                                   readonly="type == 'contact' and parent_id"
                                   context="{'country_id': country_id, 'default_country_id': country_id, 'zip': zip}"/>
                            <field name="zip" placeholder="ZIP" class="o_address_zip"
                                   readonly="type == 'contact' and parent_id"/>
                            <div name="partner_address_country" class="d-flex justify-content-between">
                                <field name="country_id" placeholder="Country" class="o_address_country"
                                       options="{&quot;no_open&quot;: True, &quot;no_create&quot;: True}"
                                       readonly="type == 'contact' and parent_id"/>
                            </div>

                        </div>

                </xpath>

                <xpath expr="//field[@name='vat']" position="before">
                    <field name="supplier_rank" invisible="1"/>

                    <field name="region_id" string="Région" readonly="type == 'contact' and parent_id" invisible="supplier_rank > 0" options="{'no_create': True, 'no_edit': True}" required="1"/>
                    <field name="city_id" string="Ville" readonly="type == 'contact' and parent_id" invisible="supplier_rank > 0" domain="[('region_id' ,'=', region_id)]" options="{'no_create': True, 'no_edit': True}" required="1"/>
                    <field name="area_id" string="Zone de livraison" readonly="type == 'contact' and parent_id" invisible="supplier_rank > 0" domain="[('city_id' ,'=', city_id)]" options="{'no_create': True, 'no_edit': True}" required="1"/>
                    <field name="common_id" invisible="supplier_rank > 0" domain="[('area_id' ,'=', area_id)]" options="{'no_create': True, 'no_edit': True}" required="1"/>
                    <field name="neighborhood_id" invisible="supplier_rank > 0" domain="[('common_id' ,'=', common_id)]" options="{'no_create': True, 'no_edit': True}" required="1"/>
                    
                    <field name="zip" string="Adresse Postal" class="o_address_zip" readonly="type == 'contact' and parent_id" invisible="supplier_rank > 0"/>
                    <field name="country_id" string="Pays" class="o_address_country" options="{&quot;no_open&quot;: True, &quot;no_create&quot;: True}" readonly="type == 'contact' and parent_id" invisible="supplier_rank > 0"/>
                    <field name="property_product_pricelist" domain="[('state' ,'=', 'done')]" invisible="supplier_rank > 0"/>
                   
                </xpath>
             
                <xpath expr="//field[@name='phone']" position="before">
                    <field name="customer_rank" invisible="1"/>
                    <field name="supplier_type" widget="radio" options="{'horizontal': True}"
                           invisible="customer_rank > 0" required="1"/>
                </xpath>
                <xpath expr="//field[@name='vat']" position="replace">
                    <field name="vat" placeholder="e.g. BE0477472701" readonly="parent_id" string="NCC"/>
                </xpath>

               <xpath expr="//group" position="after">
                    <group>
                        <group string="Information fiscale et Vente" invisible="supplier_rank > 0">
                            <field name="user_id" widget="many2one_avatar_user" string="Commercial" domain="[('share', '=', False)]" invisible="supplier_rank > 0"/>
                            <field name="property_account_position_id" invisible="supplier_rank > 0"/>
                            <field name="company_id" invisible="supplier_rank > 0"/>
                            <field name="industry_id" required="1"/>
                        </group>
                        <group string="Autres informations" invisible="supplier_rank > 0">
                            <field name="customer_type" invisible="supplier_rank > 0" required="1"/>
                            <field name="customer_profil" invisible="supplier_rank > 0" required="1"/>
                            <field name="family_cust" required="1" invisible="supplier_rank > 0"/>
                        </group>
                    </group>
               </xpath>
               <xpath expr="//page[@name='sales_purchases']" position="replace">
                    <!--attribute name="invisible">1</attribute-->
                    <page name="sales_purchases" string="Sales &amp; Purchase" invisible="customer_rank > 0">
                            <group name="container_row_2">
                                <group string="Sales" name="sale" priority="1">
                                    <field name="user_id" widget="many2one_avatar_user" domain="[('share', '=', False)]"/>
                                </group>
                                <group string="Purchase" name="purchase" priority="2">
                                </group>
                                <group name="misc" string="Misc">
                                    <field name="company_registry" invisible="parent_id"/>
                                    <field name="ref" string="Reference"/>
                                    <field name="company_id" groups="base.group_multi_company" options="{'no_create': True}" readonly="parent_id" force_save="1"/>
                                    <field name="industry_id" invisible="not is_company" options="{'no_create': True}"/>
                                    <field name="num_registre" invisible="supplier_rank > 0"/>
                                    <field name="delivery_person" widget="boolean_toggle" invisible="1"/>
                                </group>
                            </group>
                    </page>
                    
               </xpath>

            </field>
        </record>



        
        <record id="view_customer_edilac_tree" model="ir.ui.view">
            <field name="name">view.customer_edilac.tree</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <tree string="Contacts" sample="1" multi_edit="1">
                        <field name="supplier_rank" invisible="1" optional="hide"/>
                        <field name="customer_rank" invisible="1" optional="hide"/>
                        <field name="display_name" string="Nom"/>
                        <field name="user_id" optional="show" widget="many2one_avatar_user"
                               domain="[('share', '=', False)]" string="Commercial(e)"/>
                        <field name="family_cust" invisible="supplier_rank > 0"/>
                        <field name="area_id" string="Zone" invisible="supplier_rank > 0"/>
                        <field name="city_id" string="Ville"/>
                        <field name="customer_type" invisible="supplier_rank > 0"/>
                        <field name="phone" class="o_force_ltr" optional="show"/>
                        <field name="customer_profil" invisible="supplier_rank > 0"/>
                        <field name="function" column_invisible="True"/>
                        <field name="mobile" optional="hide"/>
                        <field name="email" optional="hide"/>
                        <field name="state_id" optional="hide" readonly="1"/>
                        <field name="country_id" optional="hide" readonly="1"/>
                        <field name="vat" optional="hide" readonly="1"/>
                        <field name="category_id" optional="hide" widget="many2many_tags"
                               options="{'color_field': 'color'}"/>
                        <field name="company_id" groups="base.group_multi_company" readonly="1"/>
                        <field name="is_company" column_invisible="True"/>
                        <field name="parent_id" readonly="1" column_invisible="True"/>
                        <field name="active" column_invisible="True"/>
                    </tree>
            </field>
        </record>
        

        <record id="customer_edilac_view_search" model="ir.ui.view">
            <field name="name">client.search</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_res_partner_filter"/>
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">

                <xpath expr="//search" position="replace">
                    <search string="Search Partner">
                        <field name="name"
                               filter_domain="['|', '|', '|', '|', ('complete_name', 'ilike', self), ('ref', '=', self), ('email', 'ilike', self), ('vat', 'ilike', self), ('company_registry', 'ilike', self)]"/>
                        <field name="parent_id" domain="[('is_company', '=', True)]" operator="child_of"/>
                        <field name="region_id" filter_domain="[('region_id', 'ilike', self)]" string="Région"/>
                        <field name="area_id" filter_domain="[('area_id', 'ilike', self)]" string="Zone de livraison"/>
                        <field name="common_id" filter_domain="[('common_id', 'ilike', self)]" string="Commune"/>
                        <field name="neighborhood_id" filter_domain="[('neighborhood_id', 'ilike', self)]" string="Quartier"/>
                        <field name="property_account_position_id" filter_domain="[('property_account_position_id', 'ilike', self)]" string="Position fiscale"/>
                        <field name="vat" filter_domain="[('vat', 'ilike', self)]" string="NCC"/>
                        <field name="email" filter_domain="[('email', 'ilike', self)]" string="Courriel"/>
                        <field name="phone" filter_domain="['|', ('phone', 'ilike', self), ('mobile', 'ilike', self)]"/>
                        <field name="category_id" string="Étiquettes" operator="child_of"/>
                        <field name="user_id" string="Commercial"/>
                        <separator/>
                        <filter string="Individuals" name="type_person" domain="[('is_company', '=', False)]"/>
                        <filter string="Companies" name="type_company" domain="[('is_company', '=', True)]"/>
                        <separator/>
                        <filter string="Archived" name="inactive" domain="[('active', '=', False)]"/>
                        <separator/>
                        <group expand="0" name="group_by" string="Group By">
                            <filter name="salesperson" string="Commercial" domain="[]"
                                    context="{'group_by' : 'user_id'}"/>
                            <filter name="group_region_id" string="Région" context="{'group_by': 'region_id'}"/>
                            <filter name="group_area_id" string="Zone de livraison" context="{'group_by': 'area_id'}"/> 
                            <filter name="group_common_id" string="Commune" context="{'group_by': 'common_id'}"/>
                            <filter name="group_neighborhood_id" string="Quartier" context="{'group_by': 'neighborhood_id'}"/>                
                            <filter name="customer_type" string="Type de client" context="{'group_by': 'customer_type'}"/>
                            <filter name="family_cust" string="Famille de client" context="{'group_by': 'family_cust'}"/>
                            <filter name="customer_profil" string="Profil client" context="{'group_by': 'customer_profil'}"/>
                                    
                            <filter name="group_company" string="Société" context="{'group_by': 'parent_id'}"/>
                            <filter name="group_company" string="Filiales" context="{'group_by': 'company_id'}"/>
                            <filter name="group_country" string="Pays" context="{'group_by': 'country_id'}"/>
                        </group>
                    </search>
                    

                </xpath>

            </field>
        </record>



        <record id="customers_view_action" model="ir.actions.act_window">
            <field name="name">Clients</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.partner</field>
            <field name="domain">[("customer_rank", '&gt;', 0)]</field>
             <field name="search_view_id" ref="customer_edilac_view_search"/>
            <field name="context">{'search_default_customer': 1,'res_partner_search_mode': 'customer', 'default_is_company': True, 'default_customer_rank': 1}</field>
            <field name="view_mode">tree,kanban,form</field>
        </record>

        <record id="action_tree_customer_edilac" model="ir.actions.act_window.view">
            <field name="sequence" eval="0"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_customer_edilac_tree"/>
            <field name="act_window_id" ref="customers_view_action"/>
        </record>

         

        <menuitem id="account.menu_account_customer" parent="account.menu_finance_receivables" name="Clients" action="customers_view_action" />
        <menuitem id="sale.res_partner_menu" action="customers_view_action" parent="sale.sale_order_menu" groups="sales_team.group_sale_salesman" sequence="40"/>




<!-- Region-->

        <record id="region_view_form" model="ir.ui.view">
            <field name="name">region_view_form</field>
            <field name="model">region.region</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <div class="oe_title">
                                <label for="name" string="Libellé région"/>
                                <h1>
                                    <field name="name" placeholder="Comoe"/>
                                </h1>
                                
                        </div>
                        
                        <notebook>
                            <page string="Villes">
                                <field name="city_ids">
                                    <tree>
                                        <field name="name" force_save="1"/>
                                    </tree>
                                    <form>
                                        <div class="oe_title">
                                            <div class="d-flex">
                                                <label for="name"/>
                                                <h5 class="d-flex flex-row">
                                                    <field name="name"/>
                                                </h5>
                                            </div>
                                        </div>
                                        <group>
                                            <group>
                                        
                                                <field name="region_id" invisible="1"/>
                                            </group>
                                            
                                        </group>
                                        <notebook>
                                            <page string="Zone">
                                                <field name="area_ids">
                                                    <form>
                                                        <div class="oe_title">
                                                            <h1>
                                                                <field name="name"
                                                                       placeholder="Libellé de la zone"/>
                                                            </h1>
                                                        </div>
                                                        <group>
                                                           
                                                        </group>
                                                        <notebook>
                                                            <page string="Commune">
                                                                <field name="common_ids">
                                                                    <tree editable="bottom">
                                                                        <field name="name"/>
                                                                    </tree>
                                                                </field>
                                                            </page>
                                                        </notebook>
                                                    </form>
                                                    <tree>
                                                        <field name="name"/>
                                                    </tree>
                                                </field>
                                            </page>
                                        </notebook>
                                    </form>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

